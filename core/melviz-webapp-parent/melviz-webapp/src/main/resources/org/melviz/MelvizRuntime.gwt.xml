<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.0/distro-source/core/src/gwt-module.dtd">
<module>


  <!--  Errai -->
  <inherits name="org.jboss.errai.enterprise.CDI" />
  <inherits name="org.jboss.errai.ioc.Container" />
  
  <!-- Layout Editor -->
  <inherits name="org.uberfire.UberfireAPI" />
  <inherits name="org.uberfire.ext.layout.editor.UberfireLayoutEditorClient" />
  <inherits name="org.uberfire.ext.plugin/RuntimePluginClient" />
 

  <!-- Melviz Client Deps -->
  <inherits name="org.melviz.MelvizPatternfly" />
  <inherits name="org.melviz.ServicesAPI" />
  <inherits name="org.melviz.DisplayerAPI" />

  <inherits name="org.melviz.CommonClient" />
  <inherits name="org.melviz.ServicesAPI" />
  <inherits name="org.melviz.DatasetAPI" />
  <inherits name="org.melviz.DatasetShared" />
  <inherits name="org.melviz.DatasetClient" />
  <inherits name="org.melviz.NavigationAPI" />
  <inherits name="org.melviz.DisplayerAPI" />
  <inherits name="org.melviz.DisplayerClient" />
  <inherits name="org.melviz.NavigationClient" />

  <inherits name="org.melviz.renderer.DefaultRenderer" />
  <inherits name="org.melviz.renderer.EChartsRenderer" />

  <source path="client" />
  <source path="shared" />

  <!-- We don't need to support IE10 or older -->
  <!-- There is no "ie11" permutation. IE11 uses the Firefox one (gecko1_8) -->
  <set-property name="user.agent" value="gecko1_8,safari" />

</module>
